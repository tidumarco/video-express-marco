# Server Env Variables

VIDEO_API_API_KEY=your-api-key
VIDEO_API_API_SECRET=your-api-secret
SERVER_PORT=5000

# Client Env Variables

REACT_APP_PALETTE_PRIMARY=
REACT_APP_PALETTE_SECONDARY=
REACT_APP_API_URL_DEVELOPMENT=
REACT_APP_API_URL_PRODUCTION=
REACT_APP_VERSION=$npm_package_version
REACT_APP_ASSETS_PATH = "your-server-url.com"
